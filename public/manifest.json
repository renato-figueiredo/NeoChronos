{
  "manifest_version": 3,
  "name": "NeoChronos - Focus Timer",
  "version": "1.1.0",
  "description": "Gerencie seu dia de trabalho e tarefas com um cronômetro neomórfico.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "NeoChronos"
  },
  "background": {
    "service_worker": "assets/service-worker.js",
    "type": "module"
  }
}
